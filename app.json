{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#111827",
        "resizeMode": "contain"
      },
      "splash": {
        "image": "./assets/images/iconLight.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/icon2.png",
          "resizeMode": "cover",
          "backgroundColor": "#111827"
        }
      },
      "edgeToEdgeEnabled": true,
      "package": "com.expensegauge.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-iconLight.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icon2.png",
            "resizeMode": "cover",
            "backgroundColor": "#111827"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
